<header>Tic Tac Toe</header>

	<ul class="game">
		<li class="card" data-x="0" data-y="0">
				<i class="fa"></i>
		</li>
		<li class="card" data-x="1" data-y="0">
			<i class="fa"></i>
		</li>
		<li class="card" data-x="2" data-y="0">
			<i class="fa"></i>
		</li>
		<li class="card" data-x="0" data-y="1">
			<i class="fa"></i>
		</li>
		<li class="card" data-x="1" data-y="1">
			<i class="fa"></i>
		</li>
		<li class="card" data-x="2" data-y="1">
			<i class="fa"></i>
		</li>
		<li class="card" data-x="0" data-y="2">
			<i class="fa"></i>
		</li>
		<li class="card" data-x="1" data-y="2">
			<i class="fa"></i>
		</li>
		<li class="card" data-x="2" data-y="2">
			<i class="fa"></i>
		</li>
	</ul>

<div id="controller">
	<div id="games"></div>
	<div id="message"></div>
	<button id="save">Save Game</button>
	<button id="previous">Show Previous Games</button>
	<button id="clear">Clear Current Game</button>
</div>
